{
    "name": "globalmoo/globalmoo-sdk",
    "description": "The PHP SDK for the globalMOO REST API",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.4",
        "ext-curl": "*",
        "ext-json": "*",
        "monolog/monolog": "^3.8",
        "symfony/console": "^7.2",
        "symfony/dotenv": "^7.2",
        "symfony/http-client": "^7.2",
        "symfony/serializer-pack": "^1.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "GlobalMoo\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "GlobalMoo\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Vic Cherubini",
            "email": "vic@1tomany.com"
        }
    ]
}
